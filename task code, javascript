function quadrupler(_num){
    
    return _num*4
}
console.log(quadrupler(10*4));
console.log(quadrupler(5*4));
console.log(quadrupler(3*4));
console.log(quadrupler(2*4));

function Welcome(_firstName,_lastName) {
    return `Hello ${_firstName} ${_lastName} how are you?`

}
console.log (Welcome("Nasra","Mohammed"));

